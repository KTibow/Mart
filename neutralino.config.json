{
  "applicationId": "co.skyclient.mart",
  "version": "0.0.1",
  "defaultMode": "window",
  "documentRoot": "/app/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": ["filesystem.*", "os.*", "storage.*"],
  "modes": {
    "window": {
      "title": "Mart",
      "width": 1000,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/app/assets/icons/Mart.png",
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "Mart",
    "resourcesPath": "/app/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/app/assets/js/api/neutralino.js",
    "binaryVersion": "4.5.0",
    "clientVersion": "3.4.0"
  }
}
